<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.destinyapp.biodatawalisongo">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.ScoreActivity"
            android:theme="@style/AppThemeNoAction"></activity>
        <activity android:name=".Activity.SoalActivity"
            android:label="Soal"
            android:parentActivityName=".Activity.KuisActivity"/>
        <activity
            android:name=".Activity.DetailPeninggalanActivity"
            android:parentActivityName=".Activity.PeninggalanActivity"
            android:label="Detail Peninggalan"/>
        <activity
            android:name=".Activity.DetailWalisongoActivity"
            android:parentActivityName=".Activity.BiodataActivity"
            android:label="Detail Walisongo"/>
        <activity
            android:name=".Activity.KuisActivity"
            android:parentActivityName=".Activity.MainActivity"
            android:label="Kuis"/>
        <activity
            android:name=".Activity.TentangActivity"
            android:parentActivityName=".Activity.MainActivity"
            android:label="Tentang Saya"/>
        <activity
            android:name=".Activity.PeninggalanActivity"
            android:parentActivityName=".Activity.MainActivity"
            android:label="Peninggalan Wali Songo"/>
        <activity
            android:name=".Activity.BiodataActivity"
            android:parentActivityName=".Activity.MainActivity"
            android:label="Utama"/>
        <activity
            android:name=".Activity.MainActivity"
            android:theme="@style/AppThemeNoAction" />
        <activity
            android:name=".Activity.SplashActivity"
            android:theme="@style/AppThemeNoAction">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>